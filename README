# загрузила postgres
docker pull postgres
docker run --name new_db -p 5432:5432 -ePOSTGRES_PASSWORD=12345 -e POSTGRES_HOST_AUTH_METHOD=md5 postgres

# загрузила python и установила джанго проект
docker run --network host -it python bash
docker exec -it 406e6d316d18 bash
> pip install django

# подробная информация о контейнере
docker container inspect 406e6d316d18

# эксперименты
docker run -p POSTGRES_PASSWORD=12345 5432:5432 postgres
